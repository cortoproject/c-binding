#package /test

// Basic struct
struct Point::
  x, y: int32

// Inheritance
struct Point3D: Point::
  z: int32

// Nested structs
struct Line::
  start, stop: Point

// Readonly members
struct Readonly::
  a: int32, readonly
  b: int32

// Hidden members
struct HiddenBase::
  a: int32
  b: int32, hidden

struct HiddenSub: HiddenBase, hidden::
  alias a: HiddenBase/a
  b: int32
  alias c: HiddenBase/b
  d: int32, hidden

// Optional
struct Optional::
  a: int32, optional
  b: int32

// Optional string
struct OptionalString::
  a: string, optional
  b: string

// Optional composite
struct OptionalComposite::
  a: Point, optional
  b: Point

// Optional array
struct OptionalArray::
  a: array{Point, 2}, optional
  b: array{Point, 2}

// Optional sequence
struct OptionalSequence::
  a: sequence{Point}, optional
  b: sequence{Point}

// Local members
struct Local::
  a: int32, local
  b: int32

// Private members
struct Private::
  a: int32, local
  b: int32

// Composite arguments and returntypes
struct CompositeArg::
  Point f(Point arg)

