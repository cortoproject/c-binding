#package ::class

class Foo::
    x, y: int32
    z: Foo
    int16 init()
    int16 construct()
    void destruct()
    void methodVoid()
    void methodWithArg(int32 a)
    void methodWithArgs(int32 a, int32 b)
    int32 methodWithReturn()
    int32 methodWithArgsAndReturn(int32 a, int32 b)

class HideMembers: Foo::
    x, y: int32
    z: HideMembers

class Inherit: Foo::
    a, b: int32
    c: Inherit
    int16 construct()
    void destruct()

class Modifiers::
    a, b: int32
    c: int32, private
    d: int32, private|local
    e: int32, readonly
    f: int32, hidden

class InstanceObserver::
    int16 construct()
    void onUpdate() observer
    observer onUpdateSelf: on_update
